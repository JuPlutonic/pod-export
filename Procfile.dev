# Using brew formula postgresql from Linuxbrew (`pg_ctl
#   -D | |-| | start` can't handle termination signal well).
pgsql: postgres -D  /home/linuxbrew/.linuxbrew/opt/postgresql@13/data
pod_app: bundle exec rails server -p 3000 -b 0.0.0.0
tmuxinator: /home/linuxbrew/.linuxbrew/bin/tmuxinator start pod-export
