# Using brew formula postgresql from Linuxbrew (`pg_ctl -D `$SOMEDBPATH`
# start` can't handle termination signal well, use postgres -D `$SOMEDBPATH`).
pgsql: pg_ctl start -D /home/linuxbrew/.linuxbrew/var/postgresql
pod_app: bundle exec rails server -p 3000 -b 0.0.0.0
tmuxinator: /home/linuxbrew/.linuxbrew/bin/tmuxinator start pod-export
